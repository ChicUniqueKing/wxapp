			<version>4.3.5</version>
		</dependency>
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>fastjson</artifactId>
			<version>1.2.29</version>
		</dependency>
		<!-- 日志收集 logstash -->
		<dependency>
            <groupId>net.logstash.logback</groupId>
